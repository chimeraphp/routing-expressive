{
    "name": "chimera/routing-expressive",
    "description": "Zend Expressive implementation for chimera/routing",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Lu√≠s Cobucci",
            "email": "lcobucci@gmail.com"
        }
    ],
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "require": {
        "php": "^7.4 || ^8.0",
        "chimera/routing": "^0.3",
        "zendframework/zend-diactoros": "^2.2",
        "zendframework/zend-expressive": "^3.2",
        "zendframework/zend-expressive-fastroute": "^3.0",
        "zendframework/zend-expressive-helpers": "^5.3"
    },
    "require-dev": {
        "infection/infection": "^0.16",
        "lcobucci/coding-standard": "^3.1",
        "phpstan/extension-installer": "^1.0",
        "phpstan/phpstan": "^0.12",
        "phpstan/phpstan-deprecation-rules": "^0.12",
        "phpstan/phpstan-phpunit": "^0.12",
        "phpstan/phpstan-strict-rules": "^0.12",
        "phpunit/phpunit": "^9.1"
    },
    "autoload": {
        "psr-4": {
            "Chimera\\Routing\\Expressive\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Chimera\\Routing\\Expressive\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.4.x-dev"
        }
    }
}
