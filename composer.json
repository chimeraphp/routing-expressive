{
    "name": "lcobucci/chimera-routing-expressive",
    "description": "Zend Expressive implementation for lcobucci/chimera-routing",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Lu√≠s Cobucci",
            "email": "lcobucci@gmail.com"
        }
    ],
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "require": {
        "php": "^7.1",
        "http-interop/http-middleware": "^0.4",
        "lcobucci/chimera-foundation": "1.0.x-dev@dev",
        "lcobucci/chimera-routing": "1.0.x-dev@dev",
        "ramsey/uuid": "^3.7",
        "zendframework/zend-expressive": "^2.0",
        "zendframework/zend-expressive-fastroute": "^2.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.8",
        "phpunit/phpunit": "^6.4",
        "squizlabs/php_codesniffer": "^3.1",
        "symfony/dependency-injection": "^3.3"
    },
    "suggests": {
        "symfony/dependency-injection": "In order to use the provided compiler passes"
    },
    "autoload": {
        "psr-4": {
            "Lcobucci\\Chimera\\Routing\\Expressive\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lcobucci\\Chimera\\Routing\\Expressive\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
